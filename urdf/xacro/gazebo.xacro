        <selfCollide>true</selfCollide>
    </gazebo>

    <!-- arm2 -->
    <gazebo reference="arm2">
        <gravity>true</gravity>
        <mu1>0.2</mu1>
        <mu2>0.2</mu2>
        <selfCollide>true</selfCollide>
    </gazebo>

    <!-- arm3 -->
